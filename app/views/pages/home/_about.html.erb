<section id="about">
  <div id="about-container">
    <div id="about-tondo"></div>
    
    <div id="about-cards">
      <div id="about-card-contact" class="about-card">
        <div>
          <div class="about-card-head">
            <span>Maybe you feel like reaching out</span>
          </div>
          <div class="about-card-body">
            <a id="about-contact" href="#contact">
              <span>CONTACT</span>
            </a>
          </div>
        </div>
      </div>
    </div>
    
    <div id="about-breadcrumbs">
      <div><%= render "icons/beard" %></div>
      <div><%= render "icons/leigh" %></div>
      <div><%= render "icons/hound" %></div>
      <div><%= render "icons/contact" %></div>
    </div>
  </div>
</section>
